{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "trailing-comma": [ false ],
        "quotemark": [
            true,
            "single"
        ],
        "ordered-imports": [ false ],
        "no-string-literal": false,
        "object-literal-sort-keys": false,
        "whitespace": [ false ],
        "no-var-requires": false,
        "interface-name": false
    },
    "rulesDirectory": []
}